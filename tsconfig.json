{
    "compilerOptions": {
      "strict": false, // Выключает все жесткие проверки типов
      "outDir": ".vercel_build_output",
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es2018",
      "esModuleInterop": true,
      "allowJs": true,
      "sourceMap": true,
      "moduleResolution": "node"
    },
    "include": ["**/*.ts"]
  }